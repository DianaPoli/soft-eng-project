{
  "name": "geocontrol",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon -r tsconfig-paths/register --exec ts-node src/index.ts",
    "debug-unix": "nodemon -r tsconfig-paths/register --exec 'node --inspect-brk -r ts-node/register' ./src/index.ts",
    "debug-win": "nodemon -r tsconfig-paths/register --exec \"node --inspect-brk -r ts-node/register\" ./src/index.ts",
    "generate-dto": "node scripts/generate-dto-models.js",
    "create-root": "ts-node -r tsconfig-paths/register scripts/create-root-user.ts",
    "build": "tsc",
    "test": "jest --coverage",
    "test:repo": "jest --testPathPattern='test/unit/repositories/.*\\.test\\.ts$'",
    "test:repo:db": "jest --testPathPattern='test/unit/repositories/.*\\.db\\.test\\.ts$'",
    "test:repo:db:users": "jest --testPathPattern='test/unit/repositories/UserRepository\\.db\\.test\\.ts$'",
    "test:repo:mock": "jest --testPathPattern='test/unit/repositories/.*\\.mock\\.test\\.ts$'",
    "test:e2e_debug": "jest --testPathPattern='test/e2e/.*\\.e2e\\.test\\.ts$'",
    "test:e2e": "jest --testPathPattern='test/e2e/.*\\.e2e\\.test\\.ts$' --testPathIgnorePatterns='test/e2e/networks/full/'",
    "test:e2e:users": "jest --testPathPattern='test/e2e/users/.*\\.e2e\\.test\\.ts$'",
    "test:e2e:networks": "jest --testPathPattern='test/e2e/networks/.*\\.e2e\\.test\\.ts$'",
    "test:e2e:gateways": "jest --testPathPattern='test/e2e/gateways/.*\\.e2e\\.test\\.ts$'",
    "test:e2e:sensors": "jest --testPathPattern='test/e2e/sensors/.*\\.e2e\\.test\\.ts$'",
    "test:e2e:measurements": "jest --testPathPattern='test/e2e/measurements/.*\\.e2e\\.test\\.ts$'",
    "test:integration:controllers": "jest --testPathPattern='test/integration/controllers/.*\\.integration\\.test\\.ts$'",
    "test:integration:routes": "jest --testPathPattern='test/integration/routes/.*\\.integration\\.test\\.ts$'",
    "test:integration:controllers:measurements": "jest --testPathPattern='test/integration/controllers/measurementController\\.integration\\.test\\.ts$'",
    "test:integration:routes:measurements": "jest --testPathPattern='test/integration/routes/measurementsRoutes\\.integration\\.test\\.ts$'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.4.3",
    "geocontrol": "file:",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.6",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/yamljs": "^0.2.34",
    "dependency-cruiser": "^16.10.2",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "supertest": "^7.1.0",
    "ts-jest": "^29.2.6",
    "tsconfig-paths": "^4.2.0"
  }
}
